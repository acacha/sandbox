{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.6.4",
        "acacha/admin-lte-template-laravel": "dev-master",
        "acacha/users": "dev-master",
        "barryvdh/laravel-debugbar": "^2.3",
        "barryvdh/laravel-ide-helper": "^2.3",
        "barryvdh/laravel-snappy": "^0.3.3",
        "h4cc/wkhtmltoimage-amd64": "^0.12.3",
        "h4cc/wkhtmltopdf-amd64": "^0.12.3",
        "laravel/framework": "5.4.*",
        "laravel/passport": "^2.0",
        "laravel/tinker": "~1.0",
        "predis/predis": "^1.1",
        "pusher/pusher-php-server": "^2.6",
        "scool/ebre_escool_model": "dev-master",
        "spatie/laravel-menu": "^2.1",
        "stechstudio/laravel-ssh-tunnel": "^1.1",
        "venturecraft/revisionable": "^1.28"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.7"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
